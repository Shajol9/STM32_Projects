#include <stdio.h>

int main(void)
{
	char a, b, c, d, e, f;
	printf("Enter 6 characters of your choice : ");
	fflush(stdout);
	scanf("%c %c %c %c %c %c", &a,&b,&c,&d,&e,&f);

	printf("The ascii equivalent of the input characters are %c=%d - %c=%d - %c=%d - %c=%d - %c=%d - %c=%d", a,a,b,b,c,c,d,d,e,e,f,f);
	fflush(stdout);
}

